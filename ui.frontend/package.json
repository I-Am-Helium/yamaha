{
   "name": "ui.frontend.vite",
   "private": true,
   "version": "0.0.0",
   "type": "commonjs",
   "scripts": {
      "dev": "vite",
      "build": "tsc -b && npm run lint && vite build && clientlib --verbose && node copy-assets.js && fantasticon",
      "lint": "eslint .",
      "lint:fix": "eslint . --fix",
      "fantasticon:build": "mkdir -p src/assets/generated-icons && fantasticon",
      "preview": "vite preview",
      "storybook": "npm run fantasticon:build && storybook dev -p 6006",
      "build-storybook": "npm run fantasticon:build && storybook build",
      "test-storybook": "test-storybook --watch",
      "test-storybook-coverage": "test-storybook --coverage",
      "nodemon": "nodemon",
      "aemsyncro": "aemsync -d 300 -w ../ui.apps/src/main/content",
      "ui-sync": "concurrently \"npm run nodemon\" \"npm run aemsyncro\" --kill-others",
      "build-tokens": "node src/utils/parsers/figma_to_styled_parser.mjs &&  style-dictionary build --config src/config/style-dictionary.json",
      "prepare": "cd .. && husky ui.frontend/.husky",
      "format": "prettier --write .",
      "create-token-files": "node src/utils/parsers/create_single_files.mjs",
      "generate-tokens": "node src/config/generate_tokens.mjs && node src/utils/parsers/generate-token-docs.mjs",
      "tokens": "npm run create-token-files && npm run generate-tokens"
   },
   "dependencies": {
      "fantasticon": "^1.2.3",
      "react": "^18.3.1",
      "react-dom": "^18.3.1"
   },
   "devDependencies": {
      "@adobe/react-webcomponent": "^0.1.3",
      "@babel/core": "^7.0.0",
      "@babel/plugin-proposal-class-properties": "^7.3.3",
      "@babel/plugin-proposal-decorators": "^7.25.9",
      "@babel/plugin-proposal-object-rest-spread": "^7.3.2",
      "@chromatic-com/storybook": "^3.2.6",
      "@eslint/js": "^9.19.0",
      "@storybook/addon-a11y": "^8.5.3",
      "@storybook/addon-controls": "^8.5.3",
      "@storybook/addon-coverage": "^1.0.5",
      "@storybook/addon-designs": "^8.1.0",
      "@storybook/addon-essentials": "^8.6.7",
      "@storybook/addon-highlight": "^8.5.3",
      "@storybook/addon-interactions": "^8.5.6",
      "@storybook/addon-jest": "^8.5.3",
      "@storybook/addon-measure": "^8.5.3",
      "@storybook/addon-onboarding": "^8.6.7",
      "@storybook/addon-outline": "^8.5.3",
      "@storybook/addon-storysource": "^8.5.3",
      "@storybook/addon-viewport": "^8.5.3",
      "@storybook/addon-webpack5-compiler-swc": "^2.1.0",
      "@storybook/blocks": "^8.6.7",
      "@storybook/experimental-addon-test": "^8.6.7",
      "@storybook/preset-scss": "^1.0.3",
      "@storybook/react": "^8.6.7",
      "@storybook/react-vite": "^8.6.7",
      "@storybook/react-webpack5": "^8.5.6",
      "@storybook/test": "^8.6.7",
      "@storybook/test-runner": "^0.21.3",
      "@tokens-studio/sd-transforms": "^1.2.12",
      "@types/react": "^18.3.4",
      "@types/react-dom": "^18.3.0",
      "@vitejs/plugin-react": "^4.3.4",
      "@vitest/browser": "^3.0.9",
      "@vitest/coverage-v8": "^3.0.9",
      "@whitespace/storybook-addon-html": "^6.1.1",
      "aem-clientlib-generator": "^1.8.0",
      "aemsync": "^4.0.1",
      "classnames": "^2.5.1",
      "concurrently": "^9.1.2",
      "eslint": "^9.19.0",
      "eslint-plugin-react-hooks": "^5.0.0",
      "eslint-plugin-react-refresh": "^0.4.18",
      "eslint-plugin-storybook": "^0.11.6",
      "glob": "^11.0.1",
      "globals": "^15.14.0",
      "husky": "^9.1.7",
      "nodemon": "^3.1.9",
      "playwright": "^1.51.1",
      "prettier": "^3.5.3",
      "sass": "^1.45.0",
      "storybook": "^8.6.7",
      "style-dictionary": "^4.3.3",
      "typescript": "~5.7.2",
      "typescript-eslint": "^8.22.0",
      "vite": "^6.1.0",
      "vitest": "^3.0.9"
   },
   "eslintConfig": {
      "extends": [
         "plugin:storybook/recommended"
      ]
   },
   "nodemonConfig": {
      "watch": [
         "src"
      ],
      "ext": "ts,tsx,scss",
      "exec": "npm run build",
      "ignore": [
         "src/assets/**/*"
      ]
   }
}
